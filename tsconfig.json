{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "jsx": "react",
    "outDir": "./dist",
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "allowJs": true,
    "noImplicitAny": false,
    "noImplicitThis": false
  }
}
